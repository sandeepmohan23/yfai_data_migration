  <map a="Ng4_PartRevision" b="Ng4_PartRevision">
  </map>

	<script>
		var allPartEls = from el in _xml.Elements(_ns + "Ng4_PartRevision")
			select el;
		foreach(var el in allPartEls)
		{
			el.SetAttrValue("ng4_CPP_STD", "Engineering Part");
		}
	
		XElement group1 = _xml.Elements(_ns + "Group").Where(x => x.Attribute("full_name").Value == "Confidential_Interiors_CPP.YFAI").SingleOrDefault();

		if (group1 != null)
		{
			string id = group1.GetAttrValue("elemId");
			var group1els = from el in _xml.Elements(_ns + "Ng4_PartRevision").Where(x => x.Attribute("owning_group") != null)
			where el.Attribute("owning_group").Value == ("#" + id)
			select el;

			foreach(var el in group1els)
			{
				el.SetAttrValue("ng4_CPP_STD", "Standard Part");
			}

			group1.Remove();
		}
		XElement group2 = _xml.Elements(_ns + "Group").Where(x => x.Attribute("full_name").Value == "Fasteners.YFAI").SingleOrDefault();

		if (group2 != null)
		{
			string id = group2.GetAttrValue("elemId");
			var group2els = from el in _xml.Elements(_ns + "Ng4_PartRevision").Where(x => x.Attribute("owning_group") != null)
			where el.Attribute("owning_group").Value == ("#" + id)
			select el;

			foreach(var el in group2els)
			{
				el.SetAttrValue("ng4_CPP_STD", "Standard Part");
			}

			group2.Remove();
		}
	</script>
